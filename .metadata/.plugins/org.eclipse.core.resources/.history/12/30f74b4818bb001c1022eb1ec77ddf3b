package com.png.firstclass;

public class 피포나치 {
	public static void main(String[] args) {
		int n = 3;
		int answer = 0;
		if(n==1 || n==0)  {
			answer = 1;
			return;
		}
		int a= 0;
		int b= 1;
		
		for(int i = 2 ; i <= n ; i++) {
			answer = (a+b) % 1234567;
					a = b;
			b = answer;
			
		}
		System.out.println(answer);
	}
	

}
