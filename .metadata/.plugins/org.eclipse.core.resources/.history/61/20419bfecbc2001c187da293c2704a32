package com.png.firstclass;

import java.util.Arrays;
import java.util.HashMap;
import java.util.Iterator;

public class test {
	public static void main(String[] args) {
		int n = 15;
		int answer = 0;
		
		for (int i = 1; i <= n; i++) {
			int sum =0;
			for(int j = i; j <= n; j++) {
				sum+=j;
				
				if(sum == n) {
					answer ++;
					break;
				}
				else if(sum > n) {
					break;
				}
			}
			
		}
		System.out.println(answer);
	}
}
